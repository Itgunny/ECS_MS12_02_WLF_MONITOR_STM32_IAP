    367          unsigned short MakeCrc16(unsigned char *data, unsigned short nLen)
    368          {
    369              unsigned short i = 0;
    370              unsigned short crc16 = 0;
    371              
    372              while (nLen--)
    373              {
    374                  crc16 = Crc16Table[((crc16 ^ data[i]) & 0xff)] ^ (crc16 >> 8);
    375                  i++;
    376              }
    377              
    378              return(crc16);
    379          }
